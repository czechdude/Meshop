@model Meshop.Framework.Model.Order

@{
    ViewBag.Title = "Address And Payment";
}

<h2>Address And Payment</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/messages_cs.js")" type="text/javascript"></script>


@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Shitting information</legend>

        @Html.EditorForModel()

    </fieldset>
    <fieldset>
        <legend>Shipping</legend>

        <div class="editor-label">Choose Shipping</div>
    </fieldset> 
       <fieldset>
        <legend>Payment</legend>

        <div class="editor-label">Choose Payment</div>
    </fieldset>
    <input type="submit" value="Submit Order" />
}
